<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
        @Styles.Render("~/Content/css")
        <link href="~/Content/bootstrap.rtl.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles",required:false)
</head>
<body>
    @if (User.IsInRole("System"))
    {
    <nav class="nav navbar-default">
        <div class=" container">
            <ul class="nav navbar-nav">
                <li><a href="@Url.Action("System","User")">@Fa.MainPage</a></li>
                <li><a href="@Url.Action("Index", "Staff")">@Fa.Staff</a></li>
                <li><a href="@Url.Action("Index", "Admin")">@Fa.AdminMgnt</a></li>
                <li><a href="@Url.Action("Index", "Executer")">@Fa.ExecuterMgnt</a></li>
                <li><a href="@Url.Action("Index", "College")">@Fa.CollegeMgnt</a></li>
                <li><a href="@Url.Action("Index", "EducationalGroup")">@Fa.EducationalGroupMgnt</a></li>
                <li><a href="@Url.Action("Index", "ResearchGroup")">@Fa.ResearchGroupMgnt</a></li>
                <li><a href="@Url.Action("Index", "Employer")">@Fa.EmployerMgnt</a></li>
                <li><a href="@Url.Action("Index", "Project")">@Fa.ProjectMgnt</a></li>
            </ul>
        </div>
    </nav>
    }
        <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:lightseagreen; border:0px;">
            <div class="container">
                <img src="data:image/png;base64,">
                @if (User.Identity.IsAuthenticated)
                {
                    <a href="@Url.Action("Logout", "User")" style="float:left">@Fa.Logout</a>
                }
            </div>
        </div>
    <div class="container body-content">
        @RenderBody()
        
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
